{
  "name": "vibestream-api",
  "version": "1.0.0",
  "description": "VibeStream GraphQL API with MongoDB",
  "license": "MIT",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "start:local": "export ENVIRONMENT_NAME=local && nodemon",
    "build:local": "export ENVIRONMENT_NAME=local && rm -rf dist && webpack --mode development --config webpack.dev.config.js",
    "start": "export ENVIRONMENT_NAME=production && node ./dist/main.js",
    "test": "export ENVIRONMENT_NAME=test && npx jest --silent --forceExit --detectOpenHandles --ci --coverage"
  },
  "dependencies": {
    "@apollo/server": "^4.6.0",
    "apollo-server-express": "^3.10.3",
    "body-parser": "^1.20.2",
    "cls-rtracer": "^2.6.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^16.6.0",
    "graphql-compose": "^9.0.10",
    "graphql-compose-mongoose": "^9.8.0",
    "graphql-depth-limit": "^1.1.0",
    "ioredis": "^5.3.2",
    "mongoose": "^7.0.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.3",
    "babel-jest": "^26.3.0",
    "jest": "^27.5.1",
    "jest-environment-node": "^26.3.0",
    "link-module-alias": "^1.2.0",
    "mockingoose": "^2.16.2",
    "nodemon": "^2.0.4"
  },
  "_moduleAliasIgnoreWarning": true,
  "_moduleAliases": {
    "@root": "./server",
    "@server": "./server",
    "@utils": "./server/utils",
    "@database": "./server/database",
    "@middleware": "./server/middleware",
    "@gql": "./server/gql"
  }
}
